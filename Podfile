source 'https://github.com/CocoaPods/Specs.git'
# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'ProgrammaticViews' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ProgrammaticViews
  pod 'SnapKit', '~> 3.0.2'

  target 'ProgrammaticViewsProgrammaticViewss' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ProgrammaticViewsUIProgrammaticViewss' do
    inherit! :search_paths
    # Pods for testing
  end

end
